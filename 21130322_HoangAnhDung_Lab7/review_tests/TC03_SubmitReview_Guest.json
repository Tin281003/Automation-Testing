{
	"configuration": { "device": { "orientation": "landscape", "deviceName": "Desktop" } },
	"type": "clickpath",
	"version": "1.0",
	"events": [
		{ "description": "open product page", "wait": { "waitFor": "page_complete" }, "target": {}, "type": "navigate", "url": "https://nest.botble.com/vi/products/angies-boomchickapop-sweet-salty-kettle-corn" },
		{ "description": "open reviews tab", "wait": { "waitFor": "page_complete" }, "target": { "locators": [{ "type": "css", "value": "#Reviews-tab" }] }, "type": "click", "button": 0 },
		{ "description": "click 5-star", "wait": { "waitFor": "page_complete" }, "target": { "locators": [{ "type": "css", "value": "#rating-star-5" }] }, "type": "click", "button": 0 },
		{ "description": "type guest comment", "wait": { "waitFor": "page_complete" }, "target": { "locators": [{ "type": "css", "value": "[name=\\\"comment\\\"]" }] }, "type": "keystrokes", "textValue": "Thử gửi review khi chưa đăng nhập", "simulateBlurEvent": true, "masked": false },
		{ "description": "click submit", "wait": { "waitFor": "page_complete" }, "target": { "locators": [{ "type": "dom", "value": "document.evaluate('//div[@id=\\'Reviews\\']/div/div/div[2]/form/button', document, null, XPathResult.ANY_TYPE, null).iterateNext()" }] }, "type": "click", "button": 0 }
	]
}
